{
    "manifest_version": 3,
    "name": "Phishing Email Detection",
    "description": "Detect phishing emails",
    "version": "1.0",
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ]
  }
  